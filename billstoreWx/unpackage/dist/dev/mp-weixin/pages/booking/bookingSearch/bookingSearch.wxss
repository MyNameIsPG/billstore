.cu-list-index .cu-item .cu-item-index {
  position: absolute;
  left: 0px;
  top: 0px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 60rpx;
  height: 140rpx;
  color: #666;
  font-size: 16px;
}
.cu-list-index.menu-avatar > .cu-item > .cu-avatar {
  left: 60rpx;
}
.cu-list-index.menu-avatar > .cu-item .content {
  left: 174rpx;
}
.cu-list-arrow .cu-item {
  font: normal normal normal 14px/1 uicon-iconfont;
}
.cu-list-arrow .cu-item::before {
  content: "\e605";
  color: #ddd;
}
.u-search-box {
  padding: 18rpx 30rpx;
  background: #ffffff;
  margin-bottom: 20rpx;
  position: fixed;
  left: 0px;
  top: 142rpx;
  z-index: 999;
  width: 100%;
  box-sizing: border-box;
}
page {
  background: #FFFFFF;
}
.list-type {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  padding: 0 10rpx;
}
.list-type .item {
  -webkit-box-flex: 0;
  -webkit-flex: 0 0 25%;
          flex: 0 0 25%;
  margin-bottom: 30rpx;
  padding: 0rpx 10rpx;
  box-sizing: border-box;
}
.list-type .item .item-head {
  padding-top: 10rpx;
  font-size: 36rpx;
  font-weight: bold;
  background: #eee;
  text-align: center;
}
.list-type .item .item-text {
  background: #eee;
  text-align: center;
  padding-bottom: 10rpx;
}
.list-type .item .item-text .item-text1 {
  font-size: 30rpx;
  margin-right: 10rpx;
}
.list-type .item .item-text .item-text2 {
  font-size: 26rpx;
}
.list-type .item.active .item-head {
  background: #e3e673;
}
.list-type .item.active .item-text {
  background: #e3e673;
}
.list-type .item.button {
  background: #ffffff;
}
.list-type .item.button .btns {
  background: #169bd5;
  color: #fff;
  padding: 30rpx 0;
  box-sizing: border-box;
  text-align: center;
  font-size: 32rpx;
}

